{
    "timedelta": "D",
    "date_col": "Date",
    "target": "Demand",
    "id_cols": [
        "SKU_id",
        "Store_id"
    ],
    "categorical": [
        "Promo",
        "weekday",
        "monthday"
    ],
    "real": [
        "Regular_Price",
        "Actual_Price",
        "Promo_percent"
    ],
    "model": "GRU",
    "params": {
        "LSTM": {
            "learning_rate": 0.001,
            "epochs": 2,
            "batch_size": 256,
            "weight_decay": 0.001,
            "num_hidden_units": 64,
            "gamma": 0.95,
            "dropout": 0.3
        },
        "GRU": {
            "learning_rate": 0.001,
            "epochs": 1,
            "batch_size": 256,
            "weight_decay": 0.001,
            "num_hidden_units": 64,
            "gamma": 0.95,
            "dropout": 0.3
        },
        "TFT": {
            "learning_rate": 0.001,
            "weight_decay": 0.001,
            "num_hidden_units": 64,
            "gamma": 0.95,
            "epochs": 1,
            "dropout": 0.3,
            "batch_size": 256,
            "gradient_clip_val": 0.1,
            "attention": 3,
            "hidden_continuous_size": 8
        },
        "DeepAR": {
            "learning_rate": 0.001,
            "epochs": 1,
            "batch_size": 256,
            "dropout": 0.3,
            "weight_decay": 0.001,
            "num_hidden_units": 64,
            "gradient_clip_val": 0.1
        },
        "NBeats": {
            "learning_rate": 0.001,
            "epochs": 1,
            "batch_size": 256,
            "dropout": 0.3,
            "weight_decay": 0.001,
            "gradient_clip_val": 0.1
        }
    }
}